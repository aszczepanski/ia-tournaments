<h1>Tournaments home page</h1>

<%= form_tag root_path, method: :get do %>
  <div class="row">
    <div class="span10">
    <%= text_field_tag :search, params[:search] %>
    </div>
    <div class="span2">
    <%= submit_tag "Search", name: nil, class: "btn btn-primary" %>
    </div>
  </div>
<% end %>

<%= render 'tournaments/table' %>
