<h1>Tournaments home page</h1>

<div class="row">
  <div class="span8">
    <% if Tournament.any? %>
      <h3>Tournaments (<%= Tournament.count %>)</h3>
      <ol class="tournaments">
        <%= render @tournaments %>
      </ol>
      <%= will_paginate @tournaments %>
    <% end %>
  </div>
</div>
